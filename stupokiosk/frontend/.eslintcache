 to calculate suggestions for any given input value.\nconst getSuggestions = (value, list) => {\n  const inputValue = value.trim().toLowerCase();\n  const inputLength = inputValue.length;\n\n  return inputLength === 0\n    ? []\n    : list\n        .filter((category) => {\n          if (!category.keywords) return false; // should not happen\n\n          return category.keywords.search(inputValue) > -1;\n        })\n        .slice(0, 10); // Only take first \"silce\" of results\n};\n\nexport default function Search(props) {\n  const [suggestions, setSuggestions] = useState([]);\n  const [hideSuggestions, setHideSuggestions] = useState(true);\n  const searchFocusRef = useRef(null);\n\n  const changeSearchString = (newValue) => {\n    props.setSearchString(newValue);\n    setSuggestions(getSuggestions(newValue, props.list));\n  };\n\n  function resetEscape() {\n    //setSuggestions([]);\n    setHideSuggestions(true);\n  }\n\n  document.onKeypress = function (e) {\n    e = e || window.event;\n\n    if (e.keyCode === 27) {\n      resetEscape();\n    }\n  };\n\n  useEffect(() => {\n    window.addEventListener(\"mousedown\", handleClickOutside);\n    return () => window.removeEventListener(\"mousedown\", handleClickOutside);\n  });\n\n  function handleClickOutside(e) {\n    //var isInsideSearchBar =\n    if (\n      searchFocusRef &&\n      searchFocusRef.current &&\n      !searchFocusRef.current.contains(e.target)\n    ) {\n      resetEscape();\n    }\n  }\n\n  return (\n    <div ref={searchFocusRef} className=\"relative w-1/2 sm:w-1/3\">\n      <input\n        autoFocus\n        onClick={() => setHideSuggestions(false)}\n        className=\"w-full border-solid border-2 border-blue-100 rounded py-1 pl-4 text-xl sm:text-base sm:pl-2\"\n        type=\"text\"\n        placeholder=\"Search\"\n        onChange={(e) => changeSearchString(e.target.value)}\n      />\n      {suggestions && !hideSuggestions && (\n        <div className=\"absolute cursor-pointer maxw-screen w-60 sm:w-full bg-white divide-y divide-grey-300 divide-opacity-100 sm:divide-opacity-0\">\n          {suggestions.map((suggestion, i) => {\n            return (\n              <div key={i} className=\"group\">\n                <div\n                  className=\"block text-2xl sm:text-base p-2 bg-theme-1-50 rounded-b group-hover:text-theme-1-900\"\n                  onClick={() => props.setSearchingFor(suggestion.id)}\n                >\n                  {suggestion.name}\n                </div>\n              </div>\n            );\n          })}\n        </div>\n      )}\n    </div>\n  );\n}\n","/home/wmclaughlan/dev/stupo-kiosk/stupokiosk/frontend/src/Components/Category.js",[],"/home/wmclaughlan/dev/stupo-kiosk/stupokiosk/frontend/src/Components/Product.js",[],"/home/wmclaughlan/dev/stupo-kiosk/stupokiosk/frontend/src/Components/OrderConfirmed.js",[],"/home/wmclaughlan/dev/stupo-kiosk/stupokiosk/frontend/src/Components/Checkout.js",["58","59","60"],"/home/wmclaughlan/dev/stupo-kiosk/stupokiosk/frontend/src/Components/Catalogue/Catalogue.js",[],"/home/wmclaughlan/dev/stupo-kiosk/stupokiosk/frontend/src/Components/Catalogue/CatalogueProducts.js",[],"/home/wmclaughlan/dev/stupo-kiosk/stupokiosk/frontend/src/Components/Catalogue/CatalogueCategories.js",[],"/home/wmclaughlan/dev/stupo-kiosk/stupokiosk/frontend/src/Components/Legal/PrivacyPolicy.js",[],"/home/wmclaughlan/dev/stupo-kiosk/stupokiosk/frontend/src/Components/Legal/TermsAndConditions.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":17,"column":10,"nodeType":"67","messageId":"68","endLine":17,"endColumn":22},{"ruleId":"69","severity":1,"message":"70","line":41,"column":6,"nodeType":"71","endLine":41,"endColumn":20,"suggestions":"72"},{"ruleId":"69","severity":1,"message":"73","line":82,"column":6,"nodeType":"71","endLine":82,"endColumn":28,"suggestions":"74"},{"ruleId":"65","severity":1,"message":"75","line":2,"column":10,"nodeType":"67",